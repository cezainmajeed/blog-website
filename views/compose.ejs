<%- include("partials/header"); -%>



<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="mb-3 form-group">
    <label for="exampleInputEmail1" class="form-label">Title</label>
    <input type="text" class="form-control" id="exampleInputEmail1" name="title">
  </div>
  <div class="mb-3 form-group">
  <label for="exampleFormControlTextarea1" class="form-label">Post</label>
  <textarea class="form-control" id="exampleFormControlTextarea1" rows="10" name="post"></textarea>
</div>

  <button type="submit" class="btn btn-primary mt-3">Publish</button>

</form>



